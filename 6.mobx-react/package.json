    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "core-decorators": "^0.20.0",
    "customize-cra": "^1.0.0",
    "mobx": "^6.6.1",
